{
  "name": "alchemy/security-token-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Security token bundle",
  "keywords": [],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "^4.0 || ^5.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\SecurityTokenBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\SecurityTokenBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "suggest": {
    "lcobucci/jwt": "For JWT signer"
  }
}
