{
    "name": "alchemy/metadata-manipulator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Metadata Manipulator bundle",
    "keywords": [],
    "homepage": "https://www.alchemy.fr/",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/alchemy-fr/PHPExiftool"
        },
        {
            "type": "package",
            "package": {
                "name": "exiftool/exiftool",
                "version": "12",
                "source": {
                    "url": "https://github.com/exiftool/exiftool",
                    "type": "git",
                    "reference": "12.42"
                }
            }
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/process": "^5",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^5",
        "symfony/templating": "^4.4 || ^5.0",
        "alchemy/phpexiftool": "2.1.x-dev"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "echo 'This project has no test...'"
    },
    "autoload": {
        "psr-4": {
            "Alchemy\\MetadataManipulatorBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\MetadataManipulatorBundle\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.4"
        }
    }
}
