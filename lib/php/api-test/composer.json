{
  "name": "alchemy/api-test",
  "type": "library",
  "description": "Test helper for API calls",
  "keywords": [
    "API",
    "test"
  ],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "^4.0 || ^5.0",
    "symfony/security-bundle": "^4.0 || ^5.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix",
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\ApiTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\ApiTest\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
