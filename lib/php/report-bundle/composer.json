{
    "name": "alchemy/report-bundle",
    "type": "symfony-bundle",
    "description": "Symfony report bundle",
    "keywords": [],
    "homepage": "https://www.alchemy.fr/",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../report-sdk",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0 || ^5.0 || ^6.0",
        "symfony/security-bundle": "^2.8 || ^3.0 || ^4.0 || ^5.0 || ^6.0",
        "alchemy/report-sdk": "@dev"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": [
            "vendor/bin/var-dump-check --symfony src"
        ]
    },
    "autoload": {
        "psr-4": {
            "Alchemy\\ReportBundle\\": "src"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\ReportBundle\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "php-parallel-lint/php-var-dump-check": "v0.5",
        "rector/rector": "^2.3.0"
    }
}
