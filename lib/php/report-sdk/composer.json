{
    "name": "alchemy/report-sdk",
    "type": "library",
    "description": "Symfony report SDK",
    "keywords": [],
    "homepage": "https://www.alchemy.fr/",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "opis/json-schema": "^1.0",
        "symfony/http-client": "^6"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Alchemy\\ReportSDK\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\ReportSDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": [
            "vendor/bin/var-dump-check --symfony src",
            "vendor/bin/phpunit"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "php-parallel-lint/php-var-dump-check": "v0.5",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.0.4"
    }
}
