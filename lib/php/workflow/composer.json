{
    "name": "alchemy/workflow",
    "description": "Workflow engine",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "workflow",
        "engine",
        "parallelism",
        "async"
    ],
    "autoload": {
        "psr-4": {
            "Alchemy\\Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\Workflow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": [
            "vendor/bin/var-dump-check --symfony src",
            "vendor/bin/phpunit"
        ]
    },
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "symfony/console": "^5.4 || ^6 || ^7",
        "symfony/yaml": "^6.2 || ^7",
        "ramsey/uuid": "^4.2",
        "symfony/process": "^6.3 || ^7",
        "symfony/expression-language": "^5.2 || ^6.2 || ^7",
        "symfony/property-access": "^5.2 || ^6.2 || ^7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpunit/phpunit": "^9.5",
        "symfony/http-kernel": "^6 || ^7",
        "symfony/var-dumper": "^5.4",
        "doctrine/orm": "^2.14",
        "symfony/messenger": "^6.4",
        "rector/rector": "^1.0.4",
        "colinodell/psr-testlogger": "^1.2",
        "php-parallel-lint/php-var-dump-check": "v0.5"
    }
}
