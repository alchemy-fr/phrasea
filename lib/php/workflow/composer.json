{
    "name": "alchemy/workflow",
    "description": "Workflow engine",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "workflow",
        "engine",
        "parallelism",
        "async"
    ],
    "autoload": {
        "psr-4": {
            "Alchemy\\Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\Workflow\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Arthur de Moulins",
            "email": "arthurdemoulins@gmail.com"
        }
    ],
    "scripts": {
        "rector": "vendor/bin/rector",
        "test": "./vendor/bin/phpunit",
        "cs": "vendor/bin/php-cs-fixer fix"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "symfony/console": "^5.4 || ^6",
        "symfony/yaml": "^6.2",
        "ramsey/uuid": "^4.2",
        "symfony/process": "^6.3",
        "symfony/expression-language": "^5.2 || ^6.2",
        "symfony/property-access": "^5.2 || ^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4",
        "doctrine/orm": "^2.14",
        "arthem/rabbit-bundle": "@dev",
        "rector/rector": "^0.17.1"
    }
}
