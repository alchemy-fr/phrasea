{
    "name": "alchemy/rendition-factory",
    "description": "rendition builder",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "rendition",
        "engine",
        "parallelism",
        "async"
    ],
    "autoload": {
        "psr-4": {
            "Alchemy\\RenditionFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\RenditionFactory\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alchemy"
        }
    ],
    "scripts": {
        "rector": "vendor/bin/rector",
        "test": "./vendor/bin/phpunit",
        "cs": "vendor/bin/php-cs-fixer fix"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "symfony/console": "^5.4 || ^6",
        "symfony/yaml": "^6.2",
        "ramsey/uuid": "^4.2",
        "symfony/process": "^6.3",
        "symfony/expression-language": "^5.2 || ^6.2",
        "symfony/property-access": "^5.2 || ^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4",
        "doctrine/orm": "^2.14",
        "symfony/messenger": "^6.4",
        "rector/rector": "^1.0.4",
        "colinodell/psr-testlogger": "^1.2"
    }
}
