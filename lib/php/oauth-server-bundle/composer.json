{
  "name": "alchemy/oauth-server-bundle",
  "type": "symfony-bundle",
  "description": "Symfony OAuth server bundle",
  "keywords": [],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "klapaudius/oauth-server-bundle": "^4.0",
    "symfony/framework-bundle": "^4.0 || ^5.0 || ^5 | ^6",
    "symfony/templating": "^4.0 || ^5.0 || ^6"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix",
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\OAuthServerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\OAuthServerBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.17"
  }
}
