{
  "name": "alchemy/auth-bundle",
  "type": "symfony-bundle",
  "description": "Symfony remote auth bundle",
  "keywords": [
    "User Security"
  ],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "lcobucci/jwt": "^5.0",
    "symfony/framework-bundle": "^6",
    "symfony/http-client": "^6",
    "symfony/security-bundle": "^6"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "rector": "vendor/bin/rector",
    "cs": "vendor/bin/php-cs-fixer fix",
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\AuthBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\AuthBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.17",
    "rector/rector": "^1.0.4"
  }
}
