{
    "name": "alchemy/notify-bundle",
    "type": "symfony-bundle",
    "description": "Symfony notify bundle",
    "keywords": [
        "Notification email sms"
    ],
    "homepage": "https://www.alchemy.fr/",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../auth-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.3",
        "alchemy/auth-bundle": "@dev",
        "symfony/notifier": "^6.4.13",
        "symfony/framework-bundle": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": [
            "vendor/bin/var-dump-check --symfony src"
        ]
    },
    "autoload": {
        "psr-4": {
            "Alchemy\\NotifyBundle\\": "src"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\NotifyBundle\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "php-parallel-lint/php-var-dump-check": "v0.5",
        "rector/rector": "^2.3.0"
    }
}
