{
  "name": "alchemy/rendition-factory-bundle",
  "type": "symfony-bundle",
  "description": "Symfony rendition-factory bundle",
  "keywords": [
    "rendition-factory"
  ],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^8.5",
    "symfony/framework-bundle": "^6"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "rector": "vendor/bin/rector",
    "cs": "vendor/bin/php-cs-fixer fix",
      "test": [
          "vendor/bin/var-dump-check --symfony --exclude vendor ."
      ]
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\RenditionFactoryBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\RenditionFactoryBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.17",
    "php-parallel-lint/php-var-dump-check": "v0.5",
    "rector/rector": "^2.3.0"
  }
}
