{
    "name": "alchemy/admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony admin bundle",
    "keywords": [
        "Admin"
    ],
    "homepage": "https://www.alchemy.fr/",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../auth-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.5",
        "alchemy/auth-bundle": "@dev",
        "easycorp/easyadmin-bundle": "^4.0,<=4.20.2",
        "php-amqplib/php-amqplib": "^3.7",
        "symfony/framework-bundle": "^6"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": [
            "vendor/bin/var-dump-check --symfony --exclude vendor .",
            "vendor/bin/phpunit"
        ]
    },
    "autoload": {
        "psr-4": {
            "Alchemy\\AdminBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\AdminBundle\\Tests\\": "/Tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "php-parallel-lint/php-var-dump-check": "v0.5",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^2.3.0"
    }
}
