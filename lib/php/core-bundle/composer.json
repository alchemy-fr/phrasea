{
  "name": "alchemy/core-bundle",
  "type": "symfony-bundle",
  "description": "Alchemy core bundle",
  "keywords": [],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "^4.0||^5",
    "symfony/security-bundle": "^4.0||^5",
    "symfony/monolog-bundle": "^3.5"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix",
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\CoreBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\CoreBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^8.4"
  }
}
