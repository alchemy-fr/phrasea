{
  "name": "alchemy/storage-bundle",
  "type": "symfony-bundle",
  "description": "Symfony storage bundle",
  "keywords": [],
  "homepage": "https://www.alchemy.fr/",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "aws/aws-sdk-php": "^3.20",
    "ramsey/uuid": "^3.8",
    "oneup/flysystem-bundle": "^3.1",
    "league/flysystem-aws-s3-v3": "^1.0",
    "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0 || ^5.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "echo 'This project has no test...'"
  },
  "autoload": {
    "psr-4": {
      "Alchemy\\StorageBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\StorageBundle\\Tests\\": "/Tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
