{
  "name": "@phrasea/databox-indexer",
  "version": "1.0.0",
  "description": "Consumes S3 events from AMQP and synchronize Databox",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@api-platform/client-generator": "^0.7.3",
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.13",
    "@types/minio": "^7.0.11",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "console": "node --enable-source-maps dist/console.js",
    "build": "rimraf ./dist && tsc",
    "start:dev": "nodemon",
    "test": "jest",
    "postinstall": "cd ./__lib/auth && yarn install && yarn build-commonjs && rm -rf node_modules",
    "sync-databox-types": "generate-api-platform-client --generator typescript http://databox-api src/"
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "chokidar": "^3.5.3",
    "commander": "^9.0.0",
    "express": "^4.17.1",
    "@alchemy/auth": "link:./__lib/auth",
    "minio": "^7.0.26",
    "winston": "^3.5.1"
  }
}
