    "license": "proprietary",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/alchemy-fr/PHPExiftool"
        },
        {
            "type": "package",
            "package": {
                "name": "exiftool/exiftool",
                "version": "12",
                "source": {
                    "url": "https://github.com/exiftool/exiftool",
                    "type": "git",
                    "reference": "12.42"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/4rthem/DoctrineExtensions.git"
        },
        {
            "type": "path",
            "url": "../../lib/php/core-bundle",
            "options": {
                "symlink": true
