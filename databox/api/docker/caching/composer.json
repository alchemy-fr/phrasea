        },
        {
            "type": "path",
            "url": "../../lib/php/auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/metadata-manipulator-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/workflow-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/workflow",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-sodium": "*",
        "alchemy/acl-bundle": "dev-main",
        "api-platform/core": "^3.2.7",
        "beberlei/doctrineextensions": "dev-regex-replace-flags",
        "cocur/slugify": "^4.1",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3.6",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "exercise/htmlpurifier-bundle": "^4.1",
        "friendsofsymfony/elastica-bundle": "^6.3",
        "gedmo/doctrine-extensions": "^3.5",
        "hautelook/alice-bundle": "^2.10",
        "intervention/image": "^2.7",
        "kornrunner/blurhash": "^1.2",
        "lcobucci/jwt": "^5.0",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.5",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/amqp-messenger": "^6.3",
        "symfony/asset": "^6.3",
        "symfony/console": "^5.4 || ^6",
        "symfony/dotenv": "^6.3",
        "symfony/expression-language": "^5.2 || ^6.2",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6",
        "symfony/lock": "^6.3",
        "symfony/mime": "^6|^7",
        "symfony/property-access": "^5.2 || ^6.2",
        "symfony/property-info": "^6.3",
        "symfony/psr-http-message-bridge": "^2.3",
        "symfony/rate-limiter": "^6.3",
        "symfony/security-bundle": "^6",
        "symfony/security-core": "^6.3",
        "symfony/serializer": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/validator": "^6.3",
        "symfony/yaml": "^6.2",
        "symfony/monolog-bundle": "^3.5",
        "sentry/sentry-symfony": "^4.11",
        "symfony/messenger": "^6.4",
        "guzzlehttp/guzzle": "^7.2",
        "aws/aws-sdk-php": "^3.20",
        "ramsey/uuid": "^4.2",
        "oneup/flysystem-bundle": "^4.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "easycorp/easyadmin-bundle": "^4.0",
        "symfony/http-client": "^6",
        "symfony/process": "^6.3",
        "symfony/templating": "^4.4 || ^5.0 || ^6",
        "alchemy/phpexiftool": "^4.0.1",
        "symfony/config": "^6.3"
    },
    "require-dev": {
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^3.17",
        "justinrainbow/json-schema": "^5.2",
        "lcobucci/clock": "^3.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.0.4",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/debug-bundle": "^6.3",
        "symfony/http-client": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "^6.3",
        "symfony/web-profiler-bundle": "^6.3",
        "php-parallel-lint/php-var-dump-check": "^v0.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "cache-clear": "php -d memory_limit=-1 bin/console cache:clear",
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "rector": "vendor/bin/rector",
        "phpstan": "php -d memory_limit=512M vendor/bin/phpstan analyse -c phpstan.neon",
        "cs": "vendor/bin/php-cs-fixer fix",
        "pre-phpunit": [
            "rm -f var/cache/test/data*.db",
            "bin/console -e test doctrine:schema:create",
            "bin/console -e test fos:elastica:reset"
        ],
        "phpunit": [
            "@pre-phpunit",
            "php -d memory_limit=512M ./bin/phpunit"
        ],
        "test": [
            "vendor/bin/var-dump-check --symfony src",
            "@phpstan",
            "@phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3"
        }
    }
}
