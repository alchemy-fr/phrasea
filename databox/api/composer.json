{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/alchemy-fr/PHPExiftool"
        },
        {
            "type": "package",
            "package": {
                "name": "exiftool/exiftool",
                "version": "12",
                "source": {
                    "url": "https://github.com/exiftool/exiftool",
                    "type": "git",
                    "reference": "12.42"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/4rthem/DoctrineExtensions.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/4rthem/RabbitMqBundle.git"
        },
        {
            "type": "path",
            "url": "./__lib/core-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/test-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/webhook-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/storage-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/api-test",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/oauth-server-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/metadata-manipulator-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/workflow-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/workflow",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sodium": "*",
        "alchemy/acl-bundle": "dev-main",
        "alchemy/workflow-bundle": "@dev",
        "alchemy/admin-bundle": "@dev",
        "alchemy/core-bundle": "@dev",
        "alchemy/metadata-manipulator-bundle": "@dev",
        "alchemy/oauth-server-bundle": "@dev",
        "alchemy/remote-auth-bundle": "*",
        "alchemy/storage-bundle": "*",
        "alchemy/test-bundle": "*",
        "alchemy/webhook-bundle": "@dev",
        "api-platform/core": "2.6.8",
        "arthem/rabbit-bundle": "dev-master",
        "beberlei/doctrineextensions": "dev-regex-replace-flags",
        "cocur/slugify": "^4.1",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3.3.7",
        "doctrine/doctrine-bundle": "^2.7.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "exercise/htmlpurifier-bundle": "^4.1",
        "friendsofsymfony/elastica-bundle": "^6@dev",
        "friendsofsymfony/oauth-server-bundle": "dev-master as 1.6.99",
        "gedmo/doctrine-extensions": "^3.5",
        "hautelook/alice-bundle": "^2.8",
        "kornrunner/blurhash": "^1.2",
        "lcobucci/jwt": "^4.1",
        "nelmio/cors-bundle": "^2.1",
        "php-amqplib/rabbitmq-bundle": "dev-master as 2.11.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.6,<1.7",
        "snc/redis-bundle": "^3.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^5.2.0",
        "symfony/console": "^5.2.0",
        "symfony/dotenv": "^5.2.0",
        "symfony/expression-language": "^5.2.0 || ^6.2",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.2.0",
        "symfony/mime": "^5.2.0",
        "symfony/property-access": "^5.2.0",
        "symfony/property-info": "^5.2.0",
        "symfony/rate-limiter": "^5.2.0",
        "symfony/security-bundle": "^5.2.0",
        "symfony/security-core": "^5.2,<5.4.0",
        "symfony/serializer": "^5.2.0",
        "symfony/twig-bundle": "^5.2.0",
        "symfony/validator": "^5.2.0",
        "symfony/yaml": "^5.2.0"
    },
    "require-dev": {
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^3",
        "justinrainbow/json-schema": "^5.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.25",
        "symfony/browser-kit": "^5.2.0",
        "symfony/css-selector": "^5.2.0",
        "symfony/debug-bundle": "^5.2.0",
        "symfony/http-client": "^5.2.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "cache-clear": "php -d memory_limit=-1 bin/console cache:clear",
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "rector": "vendor/bin/rector",
        "stan": "vendor/bin/phpstan analyse --memory-limit 512M src tests --level 1",
        "cs": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix",
        "test": "rm -f var/cache/test/data*.db && bin/console doctrine:schema:create -e test && bin/console fos:elastica:reset -e test && php -d 'memory_limit=512M' ./bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2.0"
        }
    }
}
