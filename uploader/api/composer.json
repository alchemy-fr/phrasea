{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/4rthem/psr-http-message-bridge.git"
        },
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/oauth-server-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/notify-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/api-test",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/acl-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/core-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/acl-bundle": "@dev",
        "alchemy/admin-bundle": "*",
        "alchemy/core-bundle": "@dev",
        "alchemy/notify-bundle": "*",
        "alchemy/oauth-server-bundle": "@dev",
        "alchemy/remote-auth-bundle": "*",
        "api-platform/api-pack": "^1.2",
        "api-platform/core": "2.4.2",
        "arthem/rabbit-bundle": "@dev",
        "arthem/request-signer-bundle": "^1.0",
        "easycorp/easyadmin-bundle": "^2.2",
        "eightpoints/guzzle-bundle": "^7.5",
        "fig/link-util": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nelmio/cors-bundle": "1.5.6",
        "nyholm/psr7": "^1.1",
        "oneup/flysystem-bundle": "^3.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "predis/predis": "^1.1",
        "ralouphie/mimey": "^2.1",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^3.2",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.4",
        "symfony/psr-http-message-bridge": "dev-https-support as 2.0.0",
        "symfony/security-bundle": "^4.4",
        "symfony/serializer-pack": "*",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-link": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "ext-json": "*",
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^2.16",
        "hautelook/alice-bundle": "^2.5",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "rm -f var/cache/test/data.db && bin/console doctrine:schema:create -e test && ./bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
