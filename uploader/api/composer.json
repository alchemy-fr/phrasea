{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "./__bundles/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__bundles/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__bundles/notify-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/admin-bundle": "*",
        "alchemy/notify-bundle": "*",
        "alchemy/remote-auth-bundle": "*",
        "api-platform/api-pack": "^1.2",
        "api-platform/core": "2.4.2",
        "arthem/rabbit-bundle": "@dev",
        "easycorp/easyadmin-bundle": "^2.2",
        "eightpoints/guzzle-bundle": "^7.5",
        "guzzlehttp/guzzle": "^6.3",
        "nyholm/psr7": "^1.1",
        "oneup/flysystem-bundle": "^3.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "ralouphie/mimey": "^2.1",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.3",
        "symfony/psr-http-message-bridge": "^1.2",
        "symfony/security-bundle": "^4.3",
        "symfony/serializer-pack": "*",
        "symfony/translation": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/web-link": "^4.3",
        "symfony/yaml": "^4.3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "ext-json": "*",
        "hautelook/alice-bundle": "^2.5",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/var-dumper": "^4.3",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.3"
        }
    }
}
