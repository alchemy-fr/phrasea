{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/4rthem/RabbitMqBundle.git"
        },
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/storage-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/oauth-server-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/notify-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/api-test",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/report-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/report-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/core-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/acl-bundle": "dev-main",
        "alchemy/admin-bundle": "@dev",
        "alchemy/core-bundle": "@dev",
        "alchemy/notify-bundle": "@dev",
        "alchemy/oauth-server-bundle": "@dev",
        "alchemy/remote-auth-bundle": "@dev",
        "alchemy/report-bundle": "@dev",
        "alchemy/storage-bundle": "@dev",
        "api-platform/core": "^2.6,<=2.6.8",
        "arthem/rabbit-bundle": "dev-master",
        "php-amqplib/rabbitmq-bundle": "dev-master as 2.11.0",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "eightpoints/guzzle-bundle": "^8.0",
        "fig/link-util": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "nelmio/cors-bundle": "^2.1",
        "nyholm/psr7": "^1.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.2.3",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "snc/redis-bundle": "^3.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/process": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/psr-http-message-bridge": "^2.1.4",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "ext-json": "*",
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^2.16",
        "hautelook/alice-bundle": "^2.5",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": "rm -f var/cache/test/data.db && bin/console doctrine:schema:create -e test && ./bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    }
}
