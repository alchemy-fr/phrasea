{
  "globalDotEnv": [".env"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false
    },
    "format": {
      "dependsOn": [
        "^format"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "build/**",
        "public/__ENV.js"
      ],
      "outputMode": "new-only",
      "dotEnv": [".env"]
    }
  }
}
