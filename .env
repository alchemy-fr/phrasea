# @category docker-compose
# Project name for Docker Compose (used as prefix for containers/networks)
COMPOSE_PROJECT_NAME=ps

# @category docker-compose
COMPOSE_FILE=docker-compose.yml
COMPOSE_PROFILES=databox,expose,uploader,db,report,tools,mailhog,dashboard,soketi,novu

# @category development
# Working directory inside the dev container
# Path inside dev containers for mounting source code
LOCAL_DIR=phrasea

# @category traefik
# Traefik Docker image
TRAEFIK_IMAGE=traefik:3.2.1

# @category traefik
# HTTPS port prefix for URLs (leave empty for default 443, include ':' otherwise)
# @example :8443
HTTPS_PORT_PREFIX=${HTTPS_PORT_PREFIX:-}

# @category traefik
# HTTPS port for Traefik reverse proxy
TRAEFIK_HTTPS_PORT=443
# HTTP port for Traefik reverse proxy
TRAEFIK_HTTP_PORT=80

# @category traefik
# Traefik dynamic config file path
TRAEFIK_PROVIDERS_FILE_FILENAME=/etc/traefik/dynamic_conf.yaml

