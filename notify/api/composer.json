{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/core-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/admin-bundle": "@dev",
        "alchemy/core-bundle": "@dev",
        "arthem/rabbit-bundle": "^0.2.3",
        "predis/predis": "^1.1",
        "qandidate/symfony-json-request-transformer": "^1.3",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.4",
        "snc/redis-bundle": "^3.2",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.4",
        "symfony/mailer": "^4.4",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "rm -f var/cache/test/data.db && bin/console doctrine:schema:create -e test && ./bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4"
        }
    }
}
