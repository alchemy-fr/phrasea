{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/4rthem/RabbitMqBundle.git"
        },
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/core-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/admin-bundle": "@dev",
        "alchemy/core-bundle": "@dev",
        "arthem/rabbit-bundle": "dev-master",
        "php-amqplib/rabbitmq-bundle": "dev-master as 2.11.0",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "predis/predis": "^1.1",
        "qandidate/symfony-json-request-transformer": "^2.2",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^6.2",
        "snc/redis-bundle": "^3.2",
        "symfony/amazon-mailer": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6.3",
        "symfony/mailer": "^6.3",
        "symfony/process": "^6.3",
        "symfony/translation": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/validator": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.17.1",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/var-dumper": "^6.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "rector": "vendor/bin/rector",
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": "rm -f var/cache/test/data.db && bin/console doctrine:schema:create -e test && ./bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3"
        }
    }
}
