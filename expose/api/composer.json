{
    "name": "alchemy/expose-api",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "./__lib/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/oauth-server-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/remote-auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/report-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/report-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./__lib/api-test",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "alchemy/admin-bundle": "@dev",
        "alchemy/report-bundle": "@dev",
        "alchemy/oauth-server-bundle": "@dev",
        "api-platform/api-pack": "^1.2",
        "doctrine/dbal": "2.9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nelmio/cors-bundle": "^1.5",
        "oneup/flysystem-bundle": "^3.1",
        "ralouphie/mimey": "^2.1",
        "ramsey/uuid-doctrine": "^1.5",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^2.16",
        "hautelook/alice-bundle": "^2.5",
        "symfony/debug-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "rm -f var/cache/test/data.db && bin/console doctrine:schema:create -e test && ./bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
