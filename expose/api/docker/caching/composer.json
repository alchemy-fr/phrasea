{
    "name": "alchemy/expose-api",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "../../lib/php/admin-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/configurator-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/storage-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/auth-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/report-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/messenger-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/report-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/core-bundle",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/api-test",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../../lib/php/notify-bundle",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "alchemy/acl-bundle": "dev-main",
        "api-platform/core": "^3.1",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^2.0",
        "doctrine/common": "^3.4.3",
        "doctrine/dbal": "^3.6",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "hautelook/alice-bundle": "^2.10",
        "exercise/htmlpurifier-bundle": "^4.0",
        "lcobucci/clock": "^3.2",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.5",
        "symfony/amqp-messenger": "^6.3",
        "symfony/asset": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/expression-language": "^6.3",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6.3",
        "symfony/property-access": "^6.3",
        "symfony/property-info": "^6.3",
        "symfony/psr-http-message-bridge": "^2.1.4",
        "symfony/security-bundle": "^6.3",
        "symfony/serializer": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/validator": "^6.3",
        "symfony/yaml": "^6.3",
        "easycorp/easyadmin-bundle": "^4.0,<=4.20.2",
        "php-amqplib/php-amqplib": "^3.7",
        "aws/aws-sdk-php": "^3.325",
        "ramsey/uuid": "^4.2.3",
        "oneup/flysystem-bundle": "^4.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "symfony/mime": "^6|^7",
        "lcobucci/jwt": "^5.0",
        "symfony/http-client": "^6",
        "symfony/messenger": "^6.4",
        "symfony/doctrine-messenger": "^6.4.7",
        "opis/json-schema": "^1.0",
        "symfony/monolog-bundle": "^3.5",
        "sentry/sentry-symfony": "^4.11",
        "symfony/novu-notifier": "^6.3",
        "symfony/notifier": "^6.4.13"
    },
    "require-dev": {
        "alchemy/api-test": "@dev",
        "friendsofphp/php-cs-fixer": "^3.17",
        "php-parallel-lint/php-var-dump-check": "v0.5",
        "phpstan/phpstan": "^2.1.33",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^2.3.0",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/debug-bundle": "^6.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "^6.3",
        "symfony/var-dumper": "^6.3",
        "symfony/web-profiler-bundle": "^6.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "cache-clear": "php -d memory_limit=-1 bin/console cache:clear",
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@cache-clear",
            "@auto-scripts"
        ],
        "rector": "vendor/bin/rector",
        "phpstan": "php -d memory_limit=512M vendor/bin/phpstan analyse -c phpstan.neon",
        "cs": "vendor/bin/php-cs-fixer fix",
        "pre-phpunit": [
            "rm -f var/cache/test/data*.db",
            "bin/console -e test doctrine:schema:create"
        ],
        "phpunit": [
            "@pre-phpunit",
            "php -d memory_limit=512M ./bin/phpunit"
        ],
        "test": [
            "vendor/bin/var-dump-check --symfony src",
            "@phpstan",
            "@phpunit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3"
        }
    }
}
